<?xml version="1.0" encoding="utf-8"?>
<LocaleMap 	xmlns="http://com.asfusion.mate/l10n"
			xmlns:mx="http://www.adobe.com/2006/mxml" 
			xmlns:factory="mx.core.*" >
	
	<mx:Metadata>
		[ResourceBundle("admin")]
	</mx:Metadata>
	
	<mx:Script>
		<![CDATA[
			import com.asfusion.intranet.admin.ui.views.Settings;
			import com.asfusion.intranet.admin.ui.views.Users;
			
			import mx.containers.FormItem;
		]]>
	</mx:Script>

	<SmartResourceInjector bundleName="admin" target="{Settings}">
		<ResourceProxy 		property="label" 				key="settings.label" />
		<ResourceProxy 		property="lblComingSoon.text" 	key="settings.message" />
	</SmartResourceInjector>
	
	<SmartResourceInjector bundleName="admin" target="{Users}">
		<ResourceProxy 		property="label" 						key="users.label" />
		<ResourceProxy 		property="lblUsers.text" 				key="users.label" />
		<ResourceProxy 		property="fiFirstName.label" 			key="users.form.firstname" />
		<ResourceProxy 		property="fiLastName.label" 			key="users.form.lastname" />
		<ResourceProxy 		property="fiPassword.label" 			key="users.form.password" />
		<ResourceProxy 		property="fiPermissions.label" 			key="users.form.permissions" />
		<ResourceProxy 		property="rbPermissionRead.label" 		key="users.form.permissions.r" />
		<ResourceProxy 		property="rbPermissionReadWrite.label" 	key="users.form.permissions.rw" />
		<ResourceProxy 		property="rbPermissionAdmin.label" 		key="users.form.permissions.admin" />
		<ResourceProxy 		property="submit.label" 				key="users.form.submit" />
		
		<ResourceProxy 		property="model.codes[0].buttonTitle" 	key="users.save" />
		<ResourceProxy 		property="model.codes[0].viewTitle" 	key="users.save.editing" />
		<ResourceProxy 		property="model.codes[1].viewTitle" 	key="users.save.new" />
	</SmartResourceInjector>

</LocaleMap>